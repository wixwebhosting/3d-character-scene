{
  "version": 2,
  "builds": [
    {
      "src": "./server.js",
      "use": "@vercel/node"
    },
    {
      "src": "./public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/log",
      "dest": "/server.js"
    },
    {
      "src": "/events",
      "dest": "/server.js"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|mp3|glb|gltf|bin))",
      "dest": "/public/$1"
    },
    {
      "src": "/",
      "dest": "/server.js"
    }
  ]
}
